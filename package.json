{
  "name": "esspec",
  "version": "0.0.1",
  "type": "module",
  "scripts": {
    "auth": "node --experimental-strip-types scripts/auth.ts",
    "create-event": "node --experimental-strip-types scripts/create-event.ts",
    "create-broadcast": "node --experimental-strip-types scripts/create-broadcast.ts",
    "download-caption": "node --experimental-strip-types scripts/download-caption.ts",
    "generate-summary": "node --experimental-strip-types scripts/generate-summary.ts",
    "generate-summaries-index": "node --experimental-strip-types scripts/generate-summaries-index.ts",
    "generate-connpass-texts": "node --experimental-strip-types scripts/generate-connpass-texts.ts",
    "setup-event": "sh -c 'pnpm run create-event \"$0\" && pnpm run create-broadcast \"$0\" && pnpm run generate-connpass-texts \"$0\"' --",
    "typecheck": "tsgo --noEmit"
  },
  "packageManager": "pnpm@10.7.1",
  "dependencies": {
    "google-auth-library": "^10.5.0",
    "googleapis": "^169.0.0",
    "js-yaml": "^4.1.1",
    "zod": "^4.3.4"
  },
  "devDependencies": {
    "@types/js-yaml": "^4.0.9",
    "@types/node": "^22.10.0",
    "@typescript/native-preview": "7.0.0-dev.20260103.1"
  }
}
